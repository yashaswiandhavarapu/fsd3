import './App.css'

function App() {
  class Person {
 private name: string;
 public age: number;
 constructor(name: string, age: number) {
 this.name = name;
 this.age = age;
 }
 public display(): void {
 console.log(`Name: ${this.name}, Age: ${this.age}`);
 }
}
let p = new Person("REKHA", 24);
p.display();}

export default App
